package com.ust.iocautowiring;

public class B {

public B() {
	// TODO Auto-generated constructor stub
}
A a;
public B(A a) {
	this.a=a;
	System.out.println("from B() constructor.....");
}
public void setA(A a) { //byName byType will use setter injection
	System.out.println("setA() called...");
	this.a=a;
}
}
